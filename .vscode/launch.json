{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Run Extract Script",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "extract"],
      "cwd": "${workspaceFolder}/model",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "outputCapture": "std",
      "env": {
        "TS_NODE_PROJECT": "${workspaceFolder}/model/tsconfig.json"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run Test Script",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "test"],
      "cwd": "${workspaceFolder}/model",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "outputCapture": "std",
      "env": {
        "TS_NODE_PROJECT": "${workspaceFolder}/model/tsconfig.json"
      }
    }
  ]
}
